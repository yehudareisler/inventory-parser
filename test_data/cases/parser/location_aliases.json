        }
      },
      "expected_rows": [
        {
          "vehicle_sub_unit": "warehouse"
        },
        {
          "vehicle_sub_unit": "C"
        }
      ],
      "expected_notes": [],
      "expected_unparseable": [],
      "assertions": {
        "row_count": 2
      }
    },
    {
      "name": "test_location_alias_doesnt_break_items",
      "input": "12 spuds to branch_c",
      "config_overrides": {
        "aliases_add": {
          "branch_c": "C"
        }
      },
      "expected_rows": [
        {
          "inv_type": "small potatoes"
        },
        {
          "vehicle_sub_unit": "C"
        }
      ],
      "expected_notes": [],
      "expected_unparseable": [],
      "assertions": {
        "row_count": 2
      }
    },
    {
